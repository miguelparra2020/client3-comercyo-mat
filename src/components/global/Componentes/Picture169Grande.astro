---
import DefaultPicture169Grande from '@img/imgsGenerales/Picture169Grande.jpg'
import { generalConfig } from "@util/generalConfig"
interface Props {
    ImagenGrandeAvif?: any
    ImagenGrandeWebp?: any
    ImagenGrandeJpg?: any
    ImagenMedianaAvif?: any
    ImagenMedianaWebp?: any
    ImagenMedianaJpg?: any
}

const { ImagenGrandeAvif,ImagenGrandeWebp, ImagenGrandeJpg, ImagenMedianaAvif,ImagenMedianaWebp, ImagenMedianaJpg=DefaultPicture169Grande } = Astro.props
---

<picture class:list={generalConfig.Home.Wellcome.ClassImagenWellcome} >
    <source 
      srcset={ImagenGrandeAvif} 
      type="image/avif" 
      media="(min-width: 1024px)" 
      class:list={generalConfig.Home.Wellcome.ClassImagenWellcome}
      style="width: 1024px"
    >
    <source 
      srcset={ImagenGrandeWebp} 
      type="image/webp" 
      media="(min-width: 1024px)" 
      class:list={generalConfig.Home.Wellcome.ClassImagenWellcome}
      style="width: 1024px"
    >
    <source 
      srcset={ImagenGrandeJpg} 
      media="(min-width: 1024px)" 
      class:list={generalConfig.Home.Wellcome.ClassImagenWellcome}
      style="width: 1024px"
    >
  
    <source 
      srcset={ImagenMedianaAvif} 
      type="image/avif" 
      media="(min-width: 600px)" 
      class:list={generalConfig.Home.Wellcome.ClassImagenWellcome}
      style="width: 600px"
    >
    <source 
      srcset={ImagenMedianaWebp} 
      type="image/webp" 
      media="(min-width: 600px)" 
      style="width: 600px"
    >
    <source 
      srcset={ImagenMedianaJpg} 
      media="(min-width: 600px)" 
      class:list={generalConfig.Home.Wellcome.ClassImagenWellcome}
      style="width: 600px"
    >
  
    <source 
      srcset={ImagenMedianaAvif} 
      type="image/avif" 
      class:list={generalConfig.Home.Wellcome.ClassImagenWellcome}
      style="width: 600px"
    >
    <source 
      srcset={ImagenMedianaWebp} 
      type="image/webp" 
      class:list={generalConfig.Home.Wellcome.ClassImagenWellcome}
      style="width: 600px"
    >
  
    <img 
      src={ImagenMedianaJpg} 
      alt="Imagen GenÃ©rica" 
      class:list={generalConfig.Home.Wellcome.ClassImagenWellcome}
      style="width: 600px" 
    >
  </picture>
  