---



<nav class="bg-white border-gray-200 ">
    <div class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4">
    <div class="flex items-center space-x-3 rtl:space-x-reverse">
      <div class="rounded-full overflow-hidden">
        <CustomImagen width={40} height={40} />
      </div>
        
        <span class="self-center text-2xl font-semibold whitespace-nowrap ">{company.name}</span>
</div>
<div class="flex items-center md:order-2 space-x-3 md:space-x-0 rtl:space-x-reverse gap-4">
  <a href="/usuario" type="button" class="flex text-sm bg-gray-800 rounded-full md:me-0 focus:ring-4 focus:ring-gray-300" aria-expanded="false">
    <span class="sr-only">Ir al perfil</span>
    <CustomImagen src={ImagenUser} width={40} height={40} class="rounded-full bg-white" alt="usario"/>
  </a>
  <a href="/usuario" type="button" class="flex text-sm bg-gray-800 rounded-full md:me-0 focus:ring-4 focus:ring-gray-300" aria-expanded="false">
    <span class="sr-only">Ir al carrito de compras</span>
    <CustomImagen src={CarShopping} width={40} height={40} class="rounded-full bg-white" alt="Carrito de compras"/>
  </a>
</div>
<div class="items-center justify-between hidden w-full md:flex md:w-auto md:order-1" id="navbar-user">
  <ul class="flex flex-col font-medium p-4 md:p-0 mt-4 border border-gray-100 rounded-lg bg-gray-50 md:space-x-8 rtl:space-x-reverse md:flex-row md:mt-0 md:border-0 md:bg-white">
    {links.map(link => ( <>
      {link.active && 
      <li class:list={`flex flex-row items-center justify-center gap-2 py-2 px-3 md:p-0 ${company.classLinks}`}>
        {link.icon && (
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class:list={['bi', link.icon]} viewBox="0 0 16 16">
            <path d={link.path1}/>
            <path d={link.path2}/>
          </svg>
        )}
        <a href={link.href} aria-current={link.text === "Inicio" ? "page" : undefined}>{link.text}</a>
      </li>}</>
    ))}
  </ul>
</div>
    </div>
  </nav>
  