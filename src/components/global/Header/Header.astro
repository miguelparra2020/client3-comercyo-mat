---
import {CustomImagen} from "@globals"
const company = {
  name: "Mi compañía",
  logo: ""
}
const links = [
  { href: "/", text: "Inicio", icon: "bi-house-door-fill", active: true,
    path1:"M6.5 14.5v-3.505c0-.245.25-.495.5-.495h2c.25 0 .5.25.5.5v3.5a.5.5 0 0 0 .5.5h4a.5.5 0 0 0 .5-.5v-7a.5.5 0 0 0-.146-.354L13 5.793V2.5a.5.5 0 0 0-.5-.5h-1a.5.5 0 0 0-.5.5v1.293L8.354 1.146a.5.5 0 0 0-.708 0l-6 6A.5.5 0 0 0 1.5 7.5v7a.5.5 0 0 0 .5.5h4a.5.5 0 0 0 .5-.5",
    path2:""
  },
  { href: "/productos", text: "Productos", icon: "bi-box-seam", active: true,
    path1:"",
    path2:""
  },
  { href: "/servicios", text: "Servicios", icon: "bi-tools", active: true,
    path1:"",
    path2:""
  },
  { href: "/fotos", text: "fotos", icon: "bi-tools", active: true,
    path1:"",
    path2:""
  },
  { href: "/publicaciones", text: "publicaciones", icon: "bi-journal-text", active: true,
    path1:"",
    path2:""
  },
  { href: "/eventos", text: "Eventos", icon: "bi-calendar-event", active: true,
    path1:"",
    path2:""
  },
  { href: "/nosotros", text: "Nosotros", icon: "bi-calendar-event", active: true,
    path1:"",
    path2:""
  },
]
---



<nav class="bg-white border-gray-200 ">
    <div class="max-w-screen-xl flex flex-wrap items-center justify-between mx-auto p-4">
    <div class="flex items-center space-x-3 rtl:space-x-reverse">
      <div class="rounded-full overflow-hidden">
        <CustomImagen width={40} height={40} />
      </div>
        
        <span class="self-center text-2xl font-semibold whitespace-nowrap ">{company.name}</span>
</div>
<div class="flex items-center md:order-2 space-x-3 md:space-x-0 rtl:space-x-reverse gap-4">
  <button type="button" class="flex text-sm bg-gray-800 rounded-full md:me-0 focus:ring-4 focus:ring-gray-300" aria-expanded="false">
    <span class="sr-only">Ir al perfil</span>
    <img class="w-8 h-8 rounded-full" src="" alt="user" />
  </button>
  <div>
    <span class="sr-only">Open user menu</span>
    <img class="w-8 h-8 rounded-full" src="" alt="user" />
  </div>
</div>
<div class="items-center justify-between hidden w-full md:flex md:w-auto md:order-1" id="navbar-user">
  <ul class="flex flex-col font-medium p-4 md:p-0 mt-4 border border-gray-100 rounded-lg bg-gray-50 md:space-x-8 rtl:space-x-reverse md:flex-row md:mt-0 md:border-0 md:bg-white">
    {links.map(link => ( <>
      {link.active && 
      <li class:list={`flex flex-row items-center justify-center gap-2 py-2 px-3 text-gray-900 rounded hover:bg-gray-100 md:hover:bg-transparent md:hover:text-blue-700 md:p-0 ${link.icon ? 'text-white bg-blue-700 md:bg-transparent md:text-blue-700' : ''}`}>
        {link.icon && (
          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class:list={['bi', link.icon]} viewBox="0 0 16 16">
            <path d={link.path1}/>
            <path d={link.path2}/>
          </svg>
        )}
        <a href={link.href} aria-current={link.text === "Inicio" ? "page" : undefined}>{link.text}</a>
      </li>}</>
    ))}
  </ul>
</div>
    </div>
  </nav>
  