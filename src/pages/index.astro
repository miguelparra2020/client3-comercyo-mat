---
// Importaciones
import {   LayoutGeneral, InicioBienvenida, InicioProductos, InicioServicios } from "@globals"
import { ModuelesActives } from "@util/generalConfig"
if (!ModuelesActives.Home) {
  return Astro.redirect('/nofound');
}
import TestLanguage  from "../components/global/Componentes/TestLanguage"
import LanguageSelector from "../components/global/Componentes/LanguageSelector"
---

    <LayoutGeneral>   
        <!-- Bienvenida -->
        <InicioBienvenida/>
        <!-- Inicio Productos -->
        <InicioProductos/>
        <!-- Inicio Servicios -->
         <InicioServicios/>
        <LanguageSelector client:idle/>
         <TestLanguage client:idle/>
    </LayoutGeneral>