---
// Importaciones
import {   LayoutGeneral, InicioBienvenida, InicioRedesSociales, InicioProductos, InicioServicios, Carrusel, ServiciosConAgendamientoInicio } from "@globals"
import { ModuelesActives } from "@util/generalConfig"
if (!ModuelesActives.Home) {
  return Astro.redirect('/nofound');
}
import LanguageSelector from "../components/global/Lenguaje/LanguageIndex"
---

    <LayoutGeneral>   
        <!-- Bienvenida -->
        <InicioBienvenida/>
        <!-- Inicio Redes Sociales -->
         <InicioRedesSociales/>
         <!-- Carrusel -->
         <Carrusel client:idle/>
        <!-- Inicio Productos -->
        <!-- <InicioProductos/> -->
        <!-- Inicio Servicios -->
         <ServiciosConAgendamientoInicio client:idle/>
        <LanguageSelector client:idle/>
    </LayoutGeneral>